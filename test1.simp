{
    translate 10 2 0

    polygon (0, 0, 100) ( 10, 0, 100) (10, 10, 100)
    polygon (0, 0, 100) (10, 10, 100) (0, 10 100)
    polygon (0, 0, 100) (0, 10, 100) (-10, 10, 100)
    polygon (0, 0, 100) (-10, 10, 100) (-10, 0, 10)
    {
	    rotate Z -150
        translate -10 10 0
        scale -1 1 1
        mesh longform
    }
    {
		rotate Z -150
        translate 10 10 0
        mesh longform
    }
}